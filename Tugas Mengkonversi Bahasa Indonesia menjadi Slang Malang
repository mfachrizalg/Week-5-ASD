#include <iostream>
#include <string>
#include <stack>

using namespace std;

int main()
{
	string a;
	stack <char> mystack;
	getline(cin,a); //agar inputan dapat terbaca ketika ada elemen ' '.
	
	for(int i = 0; i < a.length(); i++)
    	 {
	   if(a[i] == 'n' && a[i+1] == 'g') // melakukan penukaran ketika ada kata yang elemennya 'n' dan 'g', dimana 'n' lebih muncul dahulu dan tidak berjarak terhadap 'g'. Contoh kata :  "malang".
		{
		  char temp = a[i+1];
		  a[i+1] = a[i];
	 	  a[i] = temp;
		}

      	   if(a[i] == ' ') //jika kalimat tersebut terdiri lebih dari satu kata, maka kata pertama harus tetap menjadi kata pertama.
        	{
		  while(mystack.empty() == 0) //algoritma agar kata pertama tetap menjadi kata pertama.
			{
			  cout<<mystack.top();
			  mystack.pop();
			}
		  cout<<" ";
        	}
      	   mystack.push(a[i]);
    	}
  
	for(int i = 0; i < a.length(); i++) //algoritma untuk menampilkan slang malang ke terminal.
	    {
	      cout<<mystack.top();
	      mystack.pop();
	    }
  
  return 0;
}
